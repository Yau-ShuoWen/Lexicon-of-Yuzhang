<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shuowen.yuzong.dao.mapper.Character.MdrCharMapper">
    <select id="getInfo" resultType="com.shuowen.yuzong.dao.model.Character.CharMdr">
        SELECT info, id left_id
        FROM NC.mdr_char WHERE
        hanzi = #{hanzi} OR
        hanzi = #{hantz}
    </select>

</mapper>